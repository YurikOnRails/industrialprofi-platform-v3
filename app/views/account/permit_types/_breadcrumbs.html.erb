<% permit_type ||= @permit_type %>
<% team ||= @team || permit_type&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :permit_types] %>
<% if permit_type&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: permit_type.label_string, url: [:account, permit_type] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'permit_types' %>
